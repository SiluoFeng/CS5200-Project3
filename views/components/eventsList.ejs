<h2>Events</h2>
<div class="row g-3">
  <% for (let eve of events) { %>

    <div class="col-4">
      <div class="card p-1">
        <h5 class="card-title"><%= eve.eventName %></h5>
        <div class="card-text">
          <div>Date: <%= eve.date %></div>
          <div>EventType: <%= eve.eventType %></div>
          <div>
            <a class="btn btn-outline-primary" href="<%= '/events/' + eve._id + '/edit' %>">Edit</a>
            <a class="btn btn-outline-danger" href="<%= '/events/' + eve._id + '/delete' %>">Delete</a>
          </div>
        </div>
      </div>
    </div>


  <% } %>

 <%- include("./pagination.ejs", { currentPage, lastPage }) %>

</div>
